package br.com.caelum.banco;

import java.util.ArrayList;

public class TestaLista {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		ArrayList lista = new ArrayList();
		lista.add("c");
		lista.add("c#");
		lista.add("java");
		lista.add("cobol");
		System.out.println(lista);
		
		System.out.println(lista.get(2));
		
	}

}
